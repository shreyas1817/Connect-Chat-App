# Server configuration
PORT=5000
NODE_ENV=development

# MongoDB Database configuration
# Your MongoDB Atlas connection string
MONGO_URI=mongodb+srv://myuser:mysecurepassword@cluster0.mongodb.net/mydatabase?retryWrites=true&w=majority

# JWT Secret key (use a secure, randomly generated key)
JWT_SECRET=superSecretKey123!@#

# Frontend build path for production deployment
FRONTEND_BUILD_PATH=frontend/build

# Socket.io settings
SOCKET_ORIGIN=http://localhost:3000
